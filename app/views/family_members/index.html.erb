  
  <%= render 'user_details/profile' %>
   
  <div class="contentRight"> 
  
      <br/><br/><br/>

      <h3> Family Members </h3>
    <br/><br/>
    
      <table class="table table-bordered">
        <tr>
          <th> Relation </th>
          <th> First Name </th>
          <th> Last Name </th>
        </tr>
    
        <% @family_members.each do |member| %>
       
        <tr>
          <td><%= link_to member.relation.capitalize, family_member_path(:id => member.id), "data-ajax" => "false" %></td>
          <td><%= member.first_name.capitalize %></td>
          <td><%= member.last_name.capitalize %></td>
        </tr>
        <% end %>
      </table>

      <div class="text-right">
        <%= link_to "Add Member", new_family_member_path, class:"contentRightBottomHeadBlueLink", "data-ajax" => "false" %>
        &nbsp;&nbsp;&nbsp;
      </div>
    
  
  
   
  </div><!-- content right ends here -->
