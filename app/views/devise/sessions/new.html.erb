	<div class="wrapper">

		    <div class="container header">
			          <div class="logo">
				            <a href="index.html" data-ajax="false">
                      <%= image_tag "logo.png" %>
					              <!-- <img src="images/Logo.png" width="100%" alt="" title="" /> -->
				             </a>
      		        </div>
         </div>

    <hr>
    <br/><br/>
    <div class="container">
      <div class="row">
            <div class="col-md-4"></div>
        <div class="col-md-4">
          <% if alert %>
                <div class="alert alert-danger"><%= alert %>                       </div>
            <% end %>
        
        <br/>
              
                <div class="panel panel-default">
                  <div class="panel-heading"> 
                    <strong class="">LOGIN</strong>
                  </div>
                
                    <div class="panel-body">
                       <%= form_for(resource, as: resource_name, url: session_path(resource_name), :html => {:'data-ajax' => 'false'}) do |f| %>
                      
                          <div class="form-group">
                              <label for="inputEmail3" class="col-sm-3 control-label">Email</label><br/>
                        <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "E-Mail ID" %>
                       
                            </div>
                      <br/>
                            <div class="form-group">
                                <label for="inputPassword3" class="col-sm-3 control-label">Password</label><br/>
                      
                  <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "Password" %>
                  
                            </div>
                        <br/>
                          <div class="form-group last">
                           <div class="col-sm-offset-3 col-sm-9">
                                  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= f.submit "SIGN IN", class: "btn sbmbtn" %>                      
                                    <% end %>
                             <br/><br/>
                           <!--   <%= render "devise/shared/links" %> -->
                                </div>
                             </div>
                    
                    </div><!-- panel body ends here -->
               </div><!-- panel panel-default ends here -->
             </div><!-- col-md-4 ends here -->
      </div><!-- row ends here -->
    </div><!-- container ends here -->
</div><!-- wrapper ends here -->

